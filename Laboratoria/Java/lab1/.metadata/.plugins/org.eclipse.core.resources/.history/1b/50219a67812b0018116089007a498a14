package projekt;

public class Main {

public static void main(String[] args) {
TimeHistory a = new TimeHistory("first device", "short description", 4343, 1, "Volt", 6.56, 7.77);
TimeHistory b = new TimeHistory("second device", "long description", 101010, 2, "Ampere", 33.33, 123.123);
Spectrum c = new Spectrum("third device", "none", 23, 3, "Watt", 2.2, 5);
Spectrum d = new Spectrum("fourth device", "not available", 32, 4, "[-]", 0.001, 2);
Alarm e = new Alarm("fourth device", "not available", 32, 5, 1000);
Alarm f = new Alarm("fifth device", "unnecessary", 75, 6, 50);

System.out.println(a.toString() + '\n');
System.out.println(b.toString() + '\n');
System.out.println(c.toString() + '\n');
System.out.println(d.toString() + '\n');
System.out.println(e.toString() + '\n');
System.out.println(f.toString() + '\n');
}
}