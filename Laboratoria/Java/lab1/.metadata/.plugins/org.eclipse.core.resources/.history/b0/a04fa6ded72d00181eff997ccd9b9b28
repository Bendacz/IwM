package projekt;

public class Main {

public static void main(String[] args) {
Object[] p = new Object[2];
	
TimeHistory a = new TimeHistory("First Device", "Short Description", 4343, 1, "Volt", 6.56, p, 7.77);
TimeHistory b = new TimeHistory("Second Device", "Long Description", 101010, 2, "Ampere", 33.33, p, 123.123);
Spectrum c = new Spectrum("Third Device", "None", 23, 3, "Watt", 2.2, p, "Linear");
Spectrum d = new Spectrum("Fourth Device", "Not Available", 32, 4, "[-]", 0.001, p, "Logarythmic");
Alarm e = new Alarm("Fifth Device", "Not Available", 32, 5, 1000, 1);
Alarm f = new Alarm("Sixth Device", "Unnecessary", 75, 6, 50, 0);

System.out.println(a.toString() + '\n');
System.out.println(b.toString() + '\n');
System.out.println(c.toString() + '\n');
System.out.println(d.toString() + '\n');
System.out.println(e.toString() + '\n');
System.out.println(f.toString() + '\n');

}
}